{
  "name": "eslint-plugin-dak",
  "version": "1.0.0",
  "description": "ESLint Plugin with custom rules by Dak",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsup src/index.ts --no-splitting --minify",
    "test": "vitest",
    "release": "semantic-release",
    "lint": "eslint . --ext .ts"
  },
  "peerDependencies": {
    "eslint": "<=8.57.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/eslint": "8.56.10",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "eslint": "8.57.0",
    "lint-staged": "15.2.5",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.7",
    "ts-node": "10.9.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "lint --fix",
      "prettier --write"
    ],
    "*.{json,yaml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/???/eslint-plugin-xxxx.git"
  },
  "keywords": [
    "typescript",
    "template",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/???/eslint-plugin-xxxx/issues"
  },
  "homepage": "https://github.com/???/eslint-plugin-xxxx#readme",
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@typescript-eslint/rule-tester": "^7.12.0",
    "@typescript-eslint/types": "^7.12.0",
    "@typescript-eslint/utils": "^7.12.0",
    "eslint-config-next": "^14.2.3",
    "eslint-doc-generator": "^1.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "remeda": "^2.0.8"
  }
}
